# MongoDB connection string
MONGODB_URI=your_mongodb_uri

# Secret key for signing JWT tokens
JWT_SECRET=yout_secret_key

# Port number for the server
PORT=yout_port_number

# SMTP configuration for sending emails
SMTP_HOST=yout_smtp_host
SMTP_PORT=yout_smtp_port
SMTP_USER=yout_smtp_user
SMTP_PASS=yout_smtp_pass

# Google OAuth credentials
GOOGLE_CLIENT_ID=yout_google_client_id
GOOGLE_CLIENT_SECRET=yout_google_client_secret

# Facebook OAuth credentials
FACEBOOK_APP_ID=yout_facebook_app_id
FACEBOOK_APP_SECRET=yout_facebook_app_secret
FACEBOOK_CALLBACK_URL=yout_facebook_callback_url

# Google OAuth callback URL
GOOGLE_CALLBACK_URL=yout_google_callback_url

# Cloudinary configuration for file uploads
CLOUNDINARY_CLOUD_NAME=yout_cloud_name
CLOUNDINARY_API_KEY=yout_api_key
CLOUNDINARY_API_SECRET=yout_api_secret

# GitHub OAuth credentials
GITHUB_CLIENT_ID=github_client_id
GITHUB_CLIENT_SECRET=github_client_secret

# reCAPTCHA configuration
RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key